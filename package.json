{
  "name": "ember-mapbox-gl",
  "version": "2.0.0",
  "description": "Ember integration for Mapbox GL JS",
  "keywords": [
    "ember-addon"
  ],
  "homepage": "https://kturney.github.io/ember-mapbox-gl",
  "repository": "https://github.com/kturney/ember-mapbox-gl",
  "license": "MIT",
  "author": "Kyle Turney <turney.kyle@gmail.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "test:cov": "COVERAGE=true ember test --launch chrome"
  },
  "dependencies": {
    "ember-auto-import": "^2.2.4",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.6.0",
    "@embroider/test-setup": "^0.49.0",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~4.1.0",
    "ember-cli-addon-docs": "^4.0.3",
    "ember-cli-addon-docs-yuidoc": "^1.0.0",
    "ember-cli-code-coverage": "^1.0.2",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.3",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-terser": "^4.0.2",
    "ember-data": "~4.1.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-fetch": "^8.0.2",
    "ember-load-initializers": "^2.1.2",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-sinon": "^5.0.0",
    "ember-source": "~4.1.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.15.0",
    "ember-try": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-qunit": "^7.2.0",
    "loader.js": "^4.7.0",
    "mapbox-gl": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "qunit": "^2.17.2",
    "qunit-dom": "^2.0.0",
    "webpack": "^5.65.0"
  },
  "peerDependencies": {
    "mapbox-gl": "^1.0.0 || ^2.0.0"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "volta": {
    "node": "12.22.7",
    "yarn": "1.22.17"
  }
}
